package all;

import java.util.Scanner;

public class Factorial {

	public static void main(String[] args) 
	{
		// Get Factorial
		Scanner s=new Scanner(System.in);
		System.out.println("Enter a Number");
		int a=s.nextInt();
		int fact=1;
		for(int i=0;i<=a;i++)
		{
			fact=fact*i;
		}
		System.out.println(fact);
		

	}

}
