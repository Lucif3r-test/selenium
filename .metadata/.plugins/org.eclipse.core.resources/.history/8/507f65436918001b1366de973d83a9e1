package com.guru99.banking.webelements;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;

public class Customer_msg 
{
	WebDriver driver;
	//store login page elements
	By msg=By.linkText("Welcome To Customer's Page of Guru99 Bank");
	public Customer_msg(WebDriver x)
	{
		this.driver=x;
	}
	public void msg()
	{
		String msg=driver.getTitle(driver.findElement(msg));
	}
	
}
