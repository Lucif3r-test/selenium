package all;

import java.util.Arrays;

public class demo 
{
	static int print2largest(int a[], int n) 
    
	  {
		if(n>2)
		{
			Arrays.sort(a);
		}
		int i=n-1;
		return a[i]; 
		
	  }

	public static void main(String[] args) 
	{
		//To find second largest number
		  int[] a = {12, 35, 1, 10, 34, 1};
		  int n = a.length;
		  print2largest(a, n);
      
  }


	
}
 