package all;

import java.util.Arrays;

public class demo 
{
	static void print2largest(int a[], int n) 
    
	  {
		if(n>2)
		{
			Arrays.sort(a);
			System.out.println(a);
			int i=n-2;
			System.out.println(a[i]);
		}
		return; 
		
	  }

	public static void main(String[] args) 
	{
		//To find second largest number
		  int[] a = {12, 35, 1, 10, 34, 1};
		  int n = a.length;
		  print2largest(a, n);
      
  }


	
}
 