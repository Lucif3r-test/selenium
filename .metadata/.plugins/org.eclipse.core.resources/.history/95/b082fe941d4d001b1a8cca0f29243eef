package all;

public class CelciusToFarenhite {

	public static void main(String[] args) 
	{
		int c = 21;    int F =  9/5*int c + 32;        System.out.println(int F);
		int c = 21.0;  int F =  (9/5)*int c + 32;      System.out.println(int F);
		int c = 21.0;  int F =  9*int c/5 + 32;        System.out.println(int F);
		int c = 21.0;  int F =  9.*(int c/5.0) + 32;   System.out.println(int F);
		int c = 21.0;  int F =  9.0*int c/5.0 + 32;    System.out.println(int F);
		int c = 21;    int F =  9*int c/5 + 32;        System.out.println(int F);
		int c = 21.0;  int F =  (1/5)*9*int c + 32;    System.out.println(int F);
		int c = 21;    int F =  (1./5)*9*int c + 32;   System.out.println(int F);
	}

}
